<% @title = 'Preceptors' %>
<div class="page-header">
  <h1>
    <%= @title %>
    <%= link_to "Create Preceptor", new_preceptor_path, class: 'btn btn-xs btn-primary' %>
  </h1>
</div>

<%= form_tag preceptors_path, method: :get, id: "preceptors_search", class: 'form-inline' do %>
  <%= hidden_field_tag :order, params[:order] %>

  <%= text_field_tag 'search', params[:search], class: 'form-control' %>
  <%= submit_tag 'Search', class: 'btn btn-primary', name: nil %>
  <%= link_to 'Export', '#', class: 'btn btn-default', data: { object: 'export', format: 'csv', target: '#preceptors_search' } %>
<% end %>

<div id="preceptors">
  <%= render partial: 'preceptors/preceptors' %>
</div>
